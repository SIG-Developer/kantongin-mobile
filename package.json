{
  "name": "csnative",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "start": "node node_modules/react-native/local-cli/cli.js start",
    "build-android": "node node_modules/react-native/local-cli/cli.js bundle --entry-file index.android.js --bundle-output iOS/main.jsbundle --platform 'android' --assets-dest ./  --dev false --reset-cache",
    "test": "jest",
    "test:watch": "npm run test -- --watch",
    "extractLangs": "react-gettext-parser --output messages.pot 'src/**/*.js'"
  },
  "dependencies": {
    "axios": "0.18.0",
    "base-64": "0.1.0",
    "date-fns": "1.29.0",
    "gettext.js": "git+https://github.com/freiserg/gettext.js.git",
    "lodash": "4.17.10",
    "query-string": "6.1.0",
    "react": "16.3.2",
    "react-native": "0.55.4",
    "react-native-device-info": "0.22.2",
    "react-native-extended-stylesheet": "0.8.1",
    "react-native-firebase": "4.2.0",
    "react-native-keyboard-aware-scroll-view": "0.6.0",
    "react-native-navigation": "1.1.478",
    "react-native-splash-screen": "3.1.0",
    "react-native-swipeout": "2.3.4",
    "react-native-swiper": "1.5.13",
    "react-native-vector-icons": "5.0.0",
    "react-redux": "5.0.7",
    "redux": "4.0.0",
    "redux-logger": "3.0.6",
    "redux-persist": "4.10.1",
    "redux-thunk": "2.3.0",
    "shitty-qs": "1.0.1",
    "tcomb-form-native": "0.6.15"
  },
  "devDependencies": {
    "babel-eslint": "8.2.6",
    "babel-jest": "23.4.2",
    "babel-preset-react-native": "4.0.0",
    "enzyme": "3.3.0",
    "enzyme-adapter-react-16": "1.1.1",
    "eslint": "5.2.0",
    "eslint-config-airbnb": "17.0.0",
    "eslint-plugin-import": "2.13.0",
    "eslint-plugin-jsx-a11y": "6.1.1",
    "eslint-plugin-react": "7.10.0",
    "jest": "23.4.2",
    "nock": "9.4.3",
    "react-gettext-parser": "^1.5.0",
    "react-native-mock": "0.3.1",
    "react-test-renderer": "16.4.1",
    "redux-mock-store": "1.5.3"
  },
  "jest": {
    "preset": "react-native",
    "verbose": true,
    "setupFiles": [
      "<rootDir>/__tests__/mocks/global.js"
    ],
    "testMatch": [
      "**/__tests__/**/*.spec.js?(x)"
    ]
  }
}
